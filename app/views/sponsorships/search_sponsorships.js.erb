var records_div = ".sponsorships";

$( records_div ).fadeOut( "fast", function() {

    content = $("<%= escape_javascript( render 'sponsorships/animals_with_sponsorships', sponsorships: @sponsorships ) %>")

    $(records_div).fadeOut("fast", function(){
      $(records_div).empty().append(content).fadeIn("fast");
    });

});
