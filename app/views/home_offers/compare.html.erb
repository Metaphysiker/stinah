<div class="container-fluid">
  <div class="jumbotron bg-white text-justify text-darker-base-color">
    <%= matches = RequestOfferComparison.new.find_matches_for_offer(home_offer) %>
    <p>
      Platzanfrage: <%= @home_offer.id %><br />
      Anzahl Matches: <%= matches.count %>
    </p>

    <div class="row">
      <div class="col-6">
        <p>
          Platz-Angebot: <%= @home_offer.firstname %> <%= @home_offer.lastname %>
        </p>

      </div>
      <div class="col-6">
        Platz-Nachfrage-ID: <%#= @matches_for_home_offer.last.id %>

      </div>
    </div>
      <%= render 'shared/comparison_home_offer_home_request', home_offer: @home_offer, home_request: @home_request unless @home_request.nil? || @home_offer.nil? %>
      <%#= render 'shared/comparison_home_offer_home_request', home_offer: @homer_offer, home_request: @home_request %>

  </div>
</div>
