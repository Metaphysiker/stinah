<p id="notice"><%= notice %></p>

<h1>Home Offers</h1>

<table>
  <thead>
    <tr>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Street</th>
      <th>Plz</th>
      <th>City</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Year</th>
      <th>Experience</th>
      <th>Motivation</th>
      <th>Plans</th>
      <th>Species</th>
      <th>Age</th>
      <th>Size</th>
      <th>Gender</th>
      <th>Castrated</th>
      <th>Stable</th>
      <th>Stable-alt</th>
      <th>Privacy-statement</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @home_offers.each do |home_offer| %>
      <tr>
        <td><%= home_offer.firstname %></td>
        <td><%= home_offer.lastname %></td>
        <td><%= home_offer.street %></td>
        <td><%= home_offer.plz %></td>
        <td><%= home_offer.city %></td>
        <td><%= home_offer.phone %></td>
        <td><%= home_offer.email %></td>
        <td><%= home_offer.year %></td>
        <td><%= home_offer.experience %></td>
        <td><%= home_offer.motivation %></td>
        <td><%= home_offer.plans %></td>
        <td><%= home_offer.species %></td>
        <td><%= home_offer.age %></td>
        <td><%= home_offer.size %></td>
        <td><%= home_offer.gender %></td>
        <td><%= home_offer.castrated %></td>
        <td><%= home_offer.stable %></td>
        <td><%= home_offer.stable_alt %></td>
        <td><%= home_offer.privacy_statement %></td>
        <td><%= link_to 'Show', home_offer %></td>
        <td><%= link_to 'Edit', edit_home_offer_path(home_offer) %></td>
        <td><%= link_to 'Destroy', home_offer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Home Offer', new_home_offer_path %>
